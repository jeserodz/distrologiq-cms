    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@openapitools/openapi-generator-cli": "^1.0.18-5.0.0-beta2",
    "@reach/router": "^1.3.4",
    "@react-pdf/renderer": "^1.6.12",
    "@types/jest": "24.0.23",
    "@types/lodash": "^4.14.158",
    "@types/node": "12.12.8",
    "@types/reach__router": "^1.3.5",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-map-gl": "^5.2.6",
    "@types/recharts": "^1.8.14",
    "@types/styled-components": "^4.4.3",
    "@types/validator": "^10.11.3",
    "@types/yup": "^0.26.37",
    "axios": "^0.20.0",
    "class-transformer": "^0.3.1",
    "concurrently": "^5.3.0",
    "date-fns": "^2.15.0",
    "formik": "^2.1.5",
    "lodash": "^4.17.19",
    "noty": "^3.2.0-beta",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dotenv": "^0.1.2",
    "react-map-gl": "^5.2.7",
    "react-query": "^2.5.6",
    "react-query-devtools": "^2.3.2",
    "react-scripts": "^3.4.1",
    "recharts": "^1.8.5",
    "serve": "^11.3.2",
    "serve-handler": "^6.1.3",
    "styled-components": "^4.4.1",
    "typescript": "3.7.2",
    "use-debounce": "^3.4.3",
    "validator": "^12.2.0",
    "yup": "^0.27.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "react-dotenv": {
    "whitelist": [
      "API_URL"
    ]
  }
}
